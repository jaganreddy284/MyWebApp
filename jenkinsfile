pipeline {  agent any 
  stages {
        stage('Build') { 
            steps {
                echo "This is Build step."
            }
        }
        stage('Test') { 
            steps {
                echo "This is the Test step."
            }
        }
        stage('Deploy to PROD') {
            when {
              branch 'master'
            }         
            steps {
              echo "Deploying to PROD..."
            }
        }
    }
}
